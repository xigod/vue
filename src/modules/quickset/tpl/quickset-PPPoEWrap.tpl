<section id="quickSetPPPoEWrap">
  <div class="form-group">
    <label class="col-sm-4 col-md-3 control-label"></label>
    <div class="controls col-sm-7 col-md-6 col-lg-9 quickset-connect-info">This type is applicable if you have a user name and password for setting up a broadband dial-up connection. You can<span id="inportBtn" style="color:#0088cc; cursor:pointer;"> import them from your original router. </span>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-4 col-md-3 control-label">User Name</label>
    <div class="controls col-sm-7 col-md-6 col-lg-7">
      <input type="text" id="wanPPPoEUser" name="wanPPPoEUser"
            class="form-control validatebox" maxlength="128"
            required="required" data-options='{"type": "ascii"}' />
    </div>
  </div>
  <div class="form-group">
    <label class="col-sm-4 col-md-3 control-label">Password</label>
    <div class="controls col-sm-7 col-md-6 col-lg-7">
      <input type="password" id="wanPPPoEPwd" name="wanPPPoEPwd"
           class="form-control validatebox" maxlength="128"
           required="required" data-options='{"type": "ascii"}'>
    </div>
  </div>
</section>
