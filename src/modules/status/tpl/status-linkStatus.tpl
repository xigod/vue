<form class="form-horizontal" id="linkStatus">
    <h2 class="legend">Link Info</h2>
    <fieldset>
    <div class="form-group">
      <label class="col-sm-4 col-md-3 control-label">Upstream Speed</label>
      <div class="col-sm-4">
        <p id="dslUpSpeed" class="help-block">&nbsp;</p>
      </div>
    </div>
    <div class="form-group">
      <label class="col-sm-4 col-md-3 control-label">Downstream Speed</label>
      <div class="col-sm-4">
        <p id="dslDownSpeed" class="help-block">&nbsp;</p>
      </div>
    </div>
    <div class="form-group">
      <label class="col-sm-4 col-md-3 control-label">SNR Tolerance</label>
      <div class="col-sm-4">
        <p id="dslSNRCapacity" class="help-block">&nbsp;</p>
      </div>
    </div>
    <div class="form-group">
      <label class="col-sm-4 col-md-3 control-label">DSL Mode</label>
      <div class="col-sm-4">
        <p id="dslMode" class="help-block">&nbsp;</p>
      </div>
    </div>
  </fieldset>
</form>