<h2 class="legend">WiFi Signal Strength</h2>
<fieldset>
    <div id="wifiSignal" class="wifiWrap_24" style="margin-bottom: 30px;">
        <div class="form-group">
            <label class="col-sm-4 col-md-3 control-label" for="wifiPowerHigh">2.4 GHz Signal Strength</label>
            <div class="col-sm-8">
                <label id="wifiPowerHighWrap" class="radio-inline none" for="wifiPowerHigh">
                    <input type="radio" id="wifiPowerHigh" name="wifiPower" value="high" /> High </label>
                <label id="wifiPowerNormalWrap" class="radio-inline none" for="wifiPowerNormal">
                    <input type="radio" id="wifiPowerNormal" name="wifiPower" value="normal" /> Medium </label>
                <label id="wifiPowerLowerWrap" class="radio-inline none" for="wifiPowerLower">
                    <input type="radio" id="wifiPowerLower" name="wifiPower" value="low" /> Low </label>
            </div>
        </div>
    </div>
    <div id="wifiSignal5" class="wifiWrap_5">
        <div class="form-group">
            <label class="col-sm-4 col-md-3 control-label" for="wifiPowerHigh5">5 GHz Signal Strength</label>
            <div class="col-sm-8">
                <label id="wifiPowerHighWrap5" class="radio-inline none" for="wifiPowerHigh5">
                    <input type="radio" id="wifiPowerHigh5" name="wifiPower_5G" value="high" /> High </label>
                <label id="wifiPowerNormalWrap5" class="radio-inline none" for="wifiPowerNormal5">
                    <input type="radio" id="wifiPowerNormal5" name="wifiPower_5G" value="normal" /> Medium </label>
                <label id="wifiPowerLowerWrap5" class="radio-inline none" for="wifiPowerLower5">
                    <input type="radio" id="wifiPowerLower5" name="wifiPower_5G" value="low" /> Low </label>
            </div>
        </div>
    </div>
</fieldset>
<!--End Wi-Fi Signal Strength-->
